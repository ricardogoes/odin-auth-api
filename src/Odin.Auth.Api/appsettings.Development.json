{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "realm": "odin-realm",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "odin-client",
    "verify-token-audience": true,
    "credentials": {
      "secret": "__keycloak:credentials:secret__"
    },
    "confidential-port": 0,
    "RolesSource": "Realm"
  }
}