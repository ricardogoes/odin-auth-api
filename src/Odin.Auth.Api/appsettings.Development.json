{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStringsSettings": {
    "MasterConnection": "Server=.;Database=odin_master;Trusted_Connection=True;"
  },
  "KeycloakCustomerRealmSettings": {
    "realm": "__REALM_PATH__",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "odin-client",
    "verify-token-audience": true,
    "credentials": {
      "secret": "keycloak-credentials-secret",
      "admin-username": "keycloak-credentials-admin-username",
      "admin-password": "keycloak-credentials-admin-password"
    },
    "confidential-port": 0,
    "RolesSource": "Realm"
  },
  "KeycloakMasterRealmSettings": {
    "realm": "master",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "master-client",
    "verify-token-audience": true,
    "credentials": {
      "secret": "keycloak-master-credentials-secret",
      "admin-username": "keycloak-master-credentials-admin-username",
      "admin-password": "keycloak-master-credentials-admin-password"
    },
    "confidential-port": 0,
    "RolesSource": "Realm"
  }
}
